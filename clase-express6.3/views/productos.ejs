<%- include("header") %>

    <form action="/productos" method="get">
        <div>
            <h1>Productos</h1>
            <% productos.forEach((productos, index )=> { %>
                <div>
                    <a href="/productoDetalle?id=<%= productos.id %>"><p><%= productos.nombre %>, <%= productos.id %></p></a>
                </div>
                <% }) %>
        </div>
    </form>

    <%include("footer") %>